{
  "description": "todo: write me",
  "manifest_version": 2,
  "name": "typefaceless",
  "version": "1.0.0",
  "homepage_url": "https://github.com/erik/typefaceless",
  "icons": {},

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
